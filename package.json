{
  "name": "vue-app",
  "version": "0.1.0",
  "main": "server.js",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "npm run build:client && npm run build:server",
    "build:client": "vue-cli-service build",
    "build:server": "cross-env ENV_TARGET=node vue-cli-service build --no-clean",
    "start": "nodemon"
  },
  "dependencies": {
    "express": "^4.16.3",
    "vue": "^2.5.17",
    "vue-router": "^3.0.1",
    "vue-server-renderer": "^2.5.17",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.0",
    "@vue/cli-service": "^3.0.0",
    "cross-env": "^5.2.0",
    "node-sass": "^4.9.0",
    "nodemon": "^1.18.3",
    "sass-loader": "^7.0.1",
    "vue-template-compiler": "^2.5.17",
    "webpack-node-externals": "^1.7.2"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}